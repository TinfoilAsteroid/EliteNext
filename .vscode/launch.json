{
    
    "version": "0.2.0",
    "configurations": [
        {
            "type": "dezog",
            "request": "launch",
            "name": "DeZog",
            "remoteType": "cspect",
            "cspect": 
            {
                    "hostname": "localhost",
                    "port": 11000
            },
            "zxnext": 
            {
                    "baudrate": 230400,
                    "port": "/dev/tty.usbserial"
            },
            "rootFolder": "${fileDirname}",
            "listFiles": [
                {
                    "path": "H:\\OneDrive\\EliteNext\\eliteNext.lst",
                    "asm": "sjasmplus",
                    "mainFile": "H:\\OneDrive\\EliteNext\\eliteNext.asm"
                }
              ],
            "skipInterrupt": false,
            "startAutomatically": false,
            "history": {
                "reverseDebugInstructionCount": 10000,
            },
            "commandsAfterLaunch": [
                //"-sprites",
                //"-patterns"
            ],

            "topOfStack": "$7F00",
            "load": "${fileDirname}/EliteN.nex",
            "smallValuesMaximum": 513,
            "tmpDir": ".tmp"
        }      
    ]
}